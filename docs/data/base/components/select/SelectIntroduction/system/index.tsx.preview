<Select.Root>
  <SelectTrigger>Trigger</SelectTrigger>
  <Select.Backdrop />
  <Select.Positioner alignment="start" alignmentOffset={-4}>
    <SelectPopup>
      {[...Array(100)].map((_, index) => (
        <SelectItem key={index}>
          Item {index + 1}
          <SelectItemIndicator render={<Check fontSize="small" aria-hidden />} />
        </SelectItem>
      ))}
    </SelectPopup>
  </Select.Positioner>
</Select.Root>